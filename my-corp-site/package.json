{
  "name": "my-corp-site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run minify:html",
    "preview": "vite preview",
    "minify:html": "html-minifier-terser dist/index.html -o dist/index.html --collapse-whitespace --remove-comments --remove-redundant-attributes --remove-empty-attributes --minify-css true --minify-js true"
  },
  "devDependencies": {
    "html-minifier-terser": "^7.2.0",
    "sass": "^1.89.2",
    "vite": "^6.3.5",
    "vite-plugin-html-config": "^2.0.2"
  },
  "dependencies": {
    "gsap": "^3.13.0",
    "lenis": "^1.3.4",
    "lottie-web": "^5.13.0",
    "split-type": "^0.3.4"
  }
}
